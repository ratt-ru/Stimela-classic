{
    "task": "curl", 
    "base": "stimela/curl", 
    "tag": "1.0.1", 
    "description": "Download data from a remote path", 
    "prefix": "--", 
    "binary": "curl", 
    "msdir": false, 
    "parameters": [
        {
            "info": "url name", 
            "dtype": "str", 
            "default": null, 
            "required": true, 
            "name": "url"
        }, 
        {
            "default": true, 
            "dtype": "file", 
            "name": "output", 
            "io": "output"
        }, 
        {
            "info": " Client certificate file and password (SSL)", 
            "dtype": "file", 
            "default": null, 
            "name": "cert", 
            "io": "input"
        }, 
        {
            "info": "Verify the status of the server certificate", 
            "dtype": "bool", 
            "default": null, 
            "name": "cert-status"
        }, 
        {
            "info": "Verify the status of the server certificate", 
            "dtype": "str", 
            "default": null, 
            "name": "cert-type", 
            "choices": [
                "DER", 
                "PEM", 
                "ENG"
            ]
        }, 
        {
            "info": "SSL ciphers to use", 
            "dtype": "list", 
            "default": null, 
            "name": "ciphers"
        }, 
        {
            "info": "Follow redirects", 
            "dtype": "bool", 
            "default": true, 
            "name": "location"
        }, 
        {
            "info": "Read config from FILE", 
            "dtype": "file", 
            "default": null, 
            "name": "config", 
            "io": "input"
        }, 
        {
            "info": "Specify number of bytes to resume download from, alternatively specify '-' to tell curl to automatically detect", 
            "dtype": "str", 
            "default": "-", 
            "name": "continue-at"
        }
    ]
}