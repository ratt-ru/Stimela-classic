{
    "task": "casa_gencal", 
    "base": "stimela/casa", 
    "tag": "1.2.5", 
    "description": "Specify Calibration Values of Various Types", 
    "prefix": " ", 
    "binary": "gencal", 
    "msdir": true, 
    "parameters": [
        {
            "info": "Name of input visibility file", 
            "name": "msname", 
            "io": "msfile", 
            "dtype": "file", 
            "required": true, 
            "mapping": "vis"
        }, 
        {
            "info": "The new/existing calibration table", 
            "dtype": "file", 
            "default": null, 
            "name": "caltable", 
            "io": "output"
        }, 
        {
            "info": "The calibration type: 'amp','ph', 'sbd','mbd','antpos','antposvla','tsys''evlagain','opac','gc','gceff','eff'", 
            "dtype": "str", 
            "default": null, 
            "name": "caltype", 
            "choices": [
                "amp", 
                "ph", 
                "sbd", 
                "mbd", 
                "antpos", 
                "antposvla", 
                "tsys", 
                "evlagain", 
                "opac", 
                "gc", 
                "gceff", 
                "eff"
            ]
        }, 
        {
            "info": "Calibration spw(s) selection", 
            "dtype": "str", 
            "default": null, 
            "name": "spw"
        }, 
        {
            "info": "Calibration antenna(s) selection", 
            "dtype": "str", 
            "default": null, 
            "name": "antenna"
        }, 
        {
            "info": "Calibration polarizations(s) selection", 
            "dtype": "str", 
            "default": null, 
            "name": "pol"
        }, 
        {
            "info": "The calibration values", 
            "dtype": "list:float", 
            "default": null, 
            "name": "parameter"
        }
    ]
}
